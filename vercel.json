{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index-v2.html"
        },
        {
            "src": "/inmobiliarias",
            "dest": "/real-estate.html"
        },
        {
            "src": "/privacidad",
            "dest": "/privacy-policy.html"
        }
    ]
} 